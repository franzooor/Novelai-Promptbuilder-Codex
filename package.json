{
  "name": "novelai-prompt-manager",
  "version": "0.1.0",
  "description": "Electron-based NovelAI prompt and character manager",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm:dev:electron\"",
    "dev:electron": "wait-on tcp:5173 && electron -r ts-node/register/transpile-only electron/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.electron.json && vite build",
    "lint": "eslint . --ext .ts,.tsx --config .eslintrc.cjs",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "dist": "npm run build && electron-builder"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.2.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vitest": "^1.1.3",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@radix-ui/react-toast": "^1.1.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "electron-updater": "^5.3.0",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.6",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.2",
    "zustand": "^4.5.2",
    "nanoid": "^4.0.2"
  }
}